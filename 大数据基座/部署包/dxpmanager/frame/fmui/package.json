{
  "name": "ejs-m7-mobileFrame",
  "version": "7.3.6",
  "description": "适用于传统批量项目开发，与业务结合的前端框架",
  "author": "guotq",
  "license": "MIT",
  "scripts": {
    "build": "node_modules/.bin/gulp default",
    "watch": "node_modules/.bin/gulp watch",
    "dev": "npm run build && npm run watch",
    "test:eslint": "npm run eslint:report",
    "eslint": "node_modules/.bin/eslint ./js ./pages",
    "eslint:fix": "node_modules/.bin/eslint ./js ./pages --fix",
    "eslint:report": "node_modules/.bin/eslint ./js ./pages -f html -o ./_report/eslint.html",
    "webpack": "node_modules/.bin/webpack --progress --colors",
    "webpack:watch": "node_modules/.bin/webpack --watch --progress --colors"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "codemirror": "^5.42.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-clean-css": "^2.0.10",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^5.5.0",
    "gulp-connect-proxy": "^0.3.1",
    "gulp-eslint": "^4.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "jszip": "^3.1.3",
    "merge-stream": "^1.0.1",
    "mocha": "^3.5.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "gulp-babel": "^7.0.1",
    "natives": "^1.1.6",
    "sortablejs": "^1.8.0-rc1"
  }
}
