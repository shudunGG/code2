<?xml version="1.0" encoding="UTF-8"?>
<!--必须包含一个顶层元素，但是名称随意。属性说明：optional是否可选，annotation注解-->
<root>
	<filter-class annotation="第一个编码过滤器，必须放在首位">com.epoint.basic.filter.EpointEncodingFilter</filter-class>  
    <filter-class optional="true" annotation="集群过滤器 ,需要放在登录之前">com.epoint.core.utils.clustered.ClusteredSessionFilter</filter-class> 
	<filter-class optional="true" annotation="cas单点登录配置">org.jasig.cas.client.authentication.AuthenticationFilter</filter-class> 
	<filter-class optional="true" annotation="cas单点登录配置">org.jasig.cas.client.validation.Cas20ProxyReceivingTicketValidationFilter</filter-class> 
	<filter-class optional="true" annotation="cas单点登录配置">org.jasig.cas.client.util.HttpServletRequestWrapperFilter</filter-class> 
	<filter-class optional="true" annotation="cas单点登录配置">org.jasig.cas.client.util.AssertionThreadLocalFilter</filter-class>
    <filter-class optional="true" annotation="cas单点登录配置">org.jasig.cas.client.session.SingleSignOutFilter</filter-class>
	<filter-class optional="true" annotation="跨域过滤器配置">com.thetransactioncompany.cors.CORSFilter</filter-class>
	<filter-class annotation="安全框架GET过滤器">com.epoint.frame.security.filter.EpointSecurityGetFilter</filter-class>
	<filter-class annotation="框架内部通用过滤器">com.epoint.basic.filter.CommonFilter</filter-class>
	<filter-class annotation="seam框架的过滤器(控制框架登录)">com.epoint.web.session.EpointJWebFilter</filter-class>
	<filter-class annotation="F8控件库的文件上传(如果没有F8功能可以删除)">org.primefaces.webapp.filter.FileUploadFilter</filter-class>
	<filter-class annotation="F9的文件上传">com.epoint.basic.faces.fileupload.FileUploadExtFilter</filter-class>
	<filter-class annotation="安全框架POST过滤器 (放在上传后面)">com.epoint.frame.security.filter.EpointSecurityPostFilter</filter-class>
</root>