2017/4/28
-- 统一消息相关
-- 消息渠道表
create table MESSAGES_CHANNEL
(
  rowguid           VARCHAR(50),
  channelid         VARCHAR(50),
  channelname       VARCHAR(50),
  isenabletableback int,
  ordernumber       int,
  channelapiurl     VARCHAR(500),
  channelapiparam   VARCHAR(500),
  channelimpclass   VARCHAR(500),
  backapiurl     VARCHAR(500),
  backapiparam   VARCHAR(500)
);
alter table MESSAGES_CHANNEL add primary key (rowguid);

-- 消息表
create table MESSAGES_MESSAGE
(
  messageguid       VARCHAR(50),
  typeguid          VARCHAR(50),
  fromuserid        VARCHAR(50),
  fromusername      VARCHAR(50),
  targetuserid      VARCHAR(50),
  targetusername    VARCHAR(50),
  sendtime          datetime,
  readtime          datetime,
  title             VARCHAR(500),
  messagecontent    VARCHAR(2000),
  linkurl           VARCHAR(2000),
  clientidentifier  VARCHAR(200),
  clientidentifier2 VARCHAR(200),
  clientidentifier3 VARCHAR(200),
  appkey            VARCHAR(50),
  urgencylevel      int,
  status            int
);
alter table MESSAGES_MESSAGE add primary key (messageguid);

-- 消息历史表
create table MESSAGES_MESSAGEHISTORY
(
  messageguid       VARCHAR(50) not null,
  typeguid          VARCHAR(50),
  fromuserid        VARCHAR(50),
  fromusername      VARCHAR(50),
  targetuserid      VARCHAR(50),
  targetusername    VARCHAR(50),
  sendtime          datetime,
  readtime          datetime,
  title             VARCHAR(500),
  messagecontent    VARCHAR(2000),
  linkurl           VARCHAR(2000),
  clientidentifier  VARCHAR(200),
  clientidentifier2 VARCHAR(200),
  clientidentifier3 VARCHAR(200),
  appkey            VARCHAR(50),
  urgencylevel      int,
  status            int
);
alter table MESSAGES_MESSAGEHISTORY add primary key (MESSAGEGUID);

-- 个人自定义发送规则表
create table MESSAGES_PERSONALRULE
(
  rowguid     VARCHAR(50),
  userid      VARCHAR(50),
  channelguid VARCHAR(50),
  typeguid    VARCHAR(50)
);
alter table MESSAGES_PERSONALRULE add primary key (rowguid);

-- 发送规则表
create table MESSAGES_RULE
(
  rowguid         VARCHAR(50),
  channelguid     VARCHAR(50),
  typeguid        VARCHAR(50),
  messagetemplate VARCHAR(2000),
  contentlength    int,
  isenable        int
);
alter table MESSAGES_RULE add primary key (rowguid);

-- 消息类型表
create table MESSAGES_TYPE
(
  rowguid     VARCHAR(50),
  typeid      VARCHAR(50),
  typename    VARCHAR(50),
  ordernumber int,
  smallicon   VARCHAR(50),
  bigicon     VARCHAR(50)
);
alter table MESSAGES_TYPE add primary key (rowguid);

-- 消息用户表
create table MESSAGES_USER
(
  rowguid         VARCHAR(50),
  userid          VARCHAR(50),
  username        VARCHAR(50),
  channelid       VARCHAR(50),
  userchanneldata VARCHAR(500),
  adddatetime     datetime,
  lastupdatetime  datetime
);
alter table MESSAGES_USER add primary key (rowguid);

-- 消息待发表
create table MESSAGES_WAITSEND
(
  messageguid  VARCHAR(50),
  waitsendtime datetime,
  jsondata     text,
  retrycount   int
);
alter table MESSAGES_WAITSEND add primary key (messageguid);

-- 消息日志表
create table MESSAGES_LOG
(
  rowguid         VARCHAR(50),
  fromuserid      VARCHAR(50),
  fromusername    VARCHAR(50),
  targetuserid    VARCHAR(50),
  targetusername  VARCHAR(50),
  typeguid        VARCHAR(50),
  channelguid     VARCHAR(50),
  sendtime        datetime,
  waitsendtime    datetime,
  retriedcount    int,
  remindcontent   VARCHAR(2000),
  status          int
);
alter table MESSAGES_LOG add primary key (rowguid);

框架版本9.3.1(2017/4/1)

2017/3/16
--分库分表增加源数据源字段
alter table FRAME_SHARDING add sourcedb int;

2017/3/15
--增加外部方法结果表 by 季晓伟
create table workflow_method_result (
  workitemguid varchar(100) not null,
  jsonresult text,
  pviguid varchar(100) default null,
  note varchar(500) default null,
  operatedate datetime default null,
  primary key (workitemguid)
)

2017/3/14
--mis table_basicinfo 增加isenableoutds 是否启用联机数据源
alter table table_basicinfo add isenableoutds int;

2017/3/13
--工作流新增锁定机制   by 盛佳
alter table workflow_pvi add lockstatus int;
--服务表新增模块url字段
alter table FRAME_SERVICE add relationurl nvarchar(200);

2017/3/8
/*新增物理表Frame_Log(框架日志)*/
CREATE TABLE  IF NOT EXISTS FRAME_LOG(BELONGXIAQUCODE NVARCHAR(50) NULL,OPERATEUSERNAME NVARCHAR(50) NULL,OPERATEDATE datetime NULL,ROW_ID Float(10) NULL,YEARFLAG NVARCHAR(4) NULL,ROWGUID NVARCHAR(50) NOT NULL PRIMARY KEY,THREADNAME NVARCHAR(100) NULL,EVENTLEVEL NVARCHAR(50) NULL,REQUESTFILENAME NVARCHAR(100) NULL,LINE_NO NVARCHAR(50) NULL,FROMIP NVARCHAR(50) NULL,LOGINID NVARCHAR(50) NULL,USERGUID NVARCHAR(50) NULL,USERNAME NVARCHAR(50) NULL,PAGEPATH NVARCHAR(200) NULL,BUTTONNAME NVARCHAR(50) NULL,PAGEPARAMS NVARCHAR(200) NULL,OPERATOR_TYPE NVARCHAR(50) NULL,METHODNAME NVARCHAR(50) NULL,SUBSYSTEM_TYPE NVARCHAR(50) NULL,OPERATECONTENT NVARCHAR(500) NULL,OPERATECONTENTBEFORE NVARCHAR(500) NULL,OPERATETABLE NVARCHAR(50) NULL,BASEOUGUID NVARCHAR(50) NULL,OUGUID NVARCHAR(50) NULL,MACADDRESS NVARCHAR(50) NULL,RECORDDATE datetime NULL,USERDISPLAYNAME NVARCHAR(50) NULL,MODULEGUID NVARCHAR(50) NULL,MODULECODE NVARCHAR(50) NULL,MODULENAME NVARCHAR(50) NULL,MODULEURL NVARCHAR(200) NULL,SERVICENAME NVARCHAR(50) NULL,SERVICEMETHODNAME NVARCHAR(50) NULL,PROCESSTYPE NVARCHAR(50) NULL,ATTACHGUID NVARCHAR(50) NULL,CONTENT NVARCHAR(500) NULL,LOGTYPE NVARCHAR(50) NULL);

/*添加数据表frame_log_appender(frame_log_appender)*/
CREATE TABLE  IF NOT EXISTS FRAME_LOG_APPENDER(BELONGXIAQUCODE NVARCHAR(50) NULL,OPERATEUSERNAME NVARCHAR(50) NULL,OPERATEDATE datetime NULL,ROW_ID Float(10) NULL,YEARFLAG NVARCHAR(4) NULL,ROWGUID NVARCHAR(50) NOT NULL PRIMARY KEY,APPENDERNAME NVARCHAR(50) NULL,APPENDERCLASS NVARCHAR(100) NULL,FILEPATH NVARCHAR(100) NULL,CONVERSIONPATTERN NVARCHAR(100) NULL,DSNAME NVARCHAR(100) NULL,EXPORTTYPE NVARCHAR(50) NULL);

/*添加数据表FRAME_LOG_LEVEL(Frame_log_level)*/
CREATE TABLE  IF NOT EXISTS FRAME_LOG_LEVEL(LEVELGUID NVARCHAR(50) NOT NULL PRIMARY KEY,ROW_ID Float(38) NULL,LOGNAME NVARCHAR(100) NULL,LOGLEVEL NVARCHAR(5) NULL,GENERATEDATE datetime NULL,ISWORKING Float(38) NULL,DESCRIPTION NVARCHAR(500) NULL,APPENDERREF NVARCHAR(500) NULL);

