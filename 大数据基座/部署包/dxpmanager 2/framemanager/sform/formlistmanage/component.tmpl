<!-- 计算控件模板 -->
<script type="text/x-template" id="calculatecontrol-templ">
    <input type="text" 
        class="calculatecontrol"
        data-ctrl="calculatecontrol"
        id="{{id}}" 
        datafield="{{dataField}}"
        datafieldcn="{{dataField}}"
        title="{{title}}"
        itemvalue="{{ItemValue}}"
        itemprec="{{ItemPrec}}"
        conceal="{{conceal}}" 
        {{#width}} Width="{{width}}" {{/width}} 
		{{#height}} Height="{{height}}" {{/height}} 
        style="{{style}}"/>
</script>

<!-- 图片插入控件-->
<script type="text/x-template" id="epimage-templ">
    <input id="{{id}}" 
        data-ctrl="epimage"
        title="{{title}}"
        DataField="{{dataField}}" 
        DataFieldCn="{{dataField}}" 
        />
</script>

<!-- 自动布局模板 -->
<script type="text/x-template" id="layout-tb-templ">
    <table class="standard-layout" style="width:900px;margin:auto;">
        <caption style="font-size:30px;line-height:40px;font-weight:bold;text-align:center;">
            {{zhName}}
        </caption>
        <tbody>
            {{{rows}}}
        </tbody>
    </table>
</script>