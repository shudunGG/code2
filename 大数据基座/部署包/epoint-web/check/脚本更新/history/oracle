2017/4/28
-- 统一消息相关--樊志君
--消息渠道表
create table MESSAGES_CHANNEL
(
  rowguid           NVARCHAR2(50),
  channelid         NVARCHAR2(50),
  channelname       NVARCHAR2(50),
  isenabletableback INTEGER,
  ordernumber       INTEGER,
  channelapiurl     NVARCHAR2(500),
  channelapiparam   NVARCHAR2(500),
  channelimpclass   NVARCHAR2(500),
  backapiurl        NVARCHAR2(500),
  backapiparam      NVARCHAR2(500)
);
alter table MESSAGES_CHANNEL add primary key (rowguid) using index;

--消息表
create table MESSAGES_MESSAGE
(
  messageguid       NVARCHAR2(50),
  typeguid          NVARCHAR2(50),
  fromuserid        NVARCHAR2(50),
  fromusername      NVARCHAR2(50),
  targetuserid      NVARCHAR2(50),
  targetusername    NVARCHAR2(50),
  sendtime          DATE,
  readtime          DATE,
  title             NVARCHAR2(500),
  messagecontent    NVARCHAR2(2000),
  linkurl           NVARCHAR2(2000),
  clientidentifier  NVARCHAR2(200),
  clientidentifier2 NVARCHAR2(200),
  clientidentifier3 NVARCHAR2(200),
  appkey            NVARCHAR2(50),
  urgencylevel      INTEGER,
  status            INTEGER
);
alter table MESSAGES_MESSAGE add primary key (messageguid) using index;

--消息历史表
create table MESSAGES_MESSAGEHISTORY
(
  messageguid       NVARCHAR2(50) not null,
  typeguid          NVARCHAR2(50),
  fromuserid        NVARCHAR2(50),
  fromusername      NVARCHAR2(50),
  targetuserid      NVARCHAR2(50),
  targetusername    NVARCHAR2(50),
  sendtime          DATE,
  readtime          DATE,
  title             NVARCHAR2(500),
  messagecontent    NVARCHAR2(2000),
  linkurl           NVARCHAR2(2000),
  clientidentifier  NVARCHAR2(200),
  clientidentifier2 NVARCHAR2(200),
  clientidentifier3 NVARCHAR2(200),
  appkey            NVARCHAR2(50),
  urgencylevel      INTEGER,
  status            INTEGER
);
alter table MESSAGES_MESSAGEHISTORY add primary key (MESSAGEGUID) using index;

--个人自定义发送规则表
create table MESSAGES_PERSONALRULE
(
  rowguid     NVARCHAR2(50),
  userid      NVARCHAR2(50),
  channelguid NVARCHAR2(50),
  typeguid    NVARCHAR2(50)
);
alter table MESSAGES_PERSONALRULE add primary key (rowguid) using index;

--发送规则表
create table MESSAGES_RULE
(
  rowguid         NVARCHAR2(50),
  channelguid     NVARCHAR2(50),
  typeguid        NVARCHAR2(50),
  messagetemplate NVARCHAR2(2000),
  contentlength    INTEGER,
  isenable        INTEGER
);
alter table MESSAGES_RULE add primary key (rowguid) using index;

--消息类型表
create table MESSAGES_TYPE
(
  rowguid     NVARCHAR2(50),
  typeid      NVARCHAR2(50),
  typename    NVARCHAR2(50),
  ordernumber INTEGER,
  smallicon   NVARCHAR2(50),
  bigicon     NVARCHAR2(50)
);
alter table MESSAGES_TYPE add primary key (rowguid) using index;

--消息用户表
create table MESSAGES_USER
(
  rowguid         NVARCHAR2(50),
  userid          NVARCHAR2(50),
  username        NVARCHAR2(50),
  channelid       NVARCHAR2(50),
  userchanneldata NVARCHAR2(500),
  adddatetime     DATE,
  lastupdatetime  DATE
);
alter table MESSAGES_USER add primary key (rowguid) using index;

--消息待发表
create table MESSAGES_WAITSEND
(
  messageguid  NVARCHAR2(50),
  waitsendtime DATE,
  jsondata     NCLOB,
  retrycount   INTEGER
);
alter table MESSAGES_WAITSEND add primary key (messageguid) using index;

-- 消息日志表
create table MESSAGES_LOG
(
  rowguid        NVARCHAR2(50),
  fromuserid     NVARCHAR2(50),
  fromusername   NVARCHAR2(50),
  targetuserid   NVARCHAR2(50),
  targetusername NVARCHAR2(50),
  typeguid       NVARCHAR2(50),
  channelguid    NVARCHAR2(50),
  sendtime       DATE,
  waitsendtime   DATE,
  retriedcount   INTEGER,
  remindcontent  NVARCHAR2(2000),
  status         INTEGER
);
alter table MESSAGES_LOG add primary key (rowguid) using index;


