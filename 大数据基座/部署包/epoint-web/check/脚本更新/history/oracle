2017/3/8
--记录数据表Frame_Log(框架日志)
declare IsExist number;
begin
--创建数据表FRAME_LOG
execute immediate  ' CREATE TABLE FRAME_LOG(BELONGXIAQUCODE NVARCHAR2(50) NULL,OPERATEUSERNAME NVARCHAR2(50) NULL,OPERATEDATE DATE NULL,ROW_ID NUMBER(10) NULL,YEARFLAG NVARCHAR2(4) NULL,ROWGUID NVARCHAR2(50) NOT NULL constraint pk_ROWGUID PRIMARY KEY,THREADNAME NVARCHAR2(100) NULL,EVENTLEVEL NVARCHAR2(50) NULL,REQUESTFILENAME NVARCHAR2(100) NULL,LINE_NO NVARCHAR2(50) NULL,FROMIP NVARCHAR2(50) NULL,LOGINID NVARCHAR2(50) NULL,USERGUID NVARCHAR2(50) NULL,USERNAME NVARCHAR2(50) NULL,PAGEPATH NVARCHAR2(200) NULL,BUTTONNAME NVARCHAR2(50) NULL,PAGEPARAMS NVARCHAR2(200) NULL,OPERATOR_TYPE NVARCHAR2(50) NULL,METHODNAME NVARCHAR2(50) NULL,SUBSYSTEM_TYPE NVARCHAR2(50) NULL,OPERATECONTENT NVARCHAR2(500) NULL,OPERATECONTENTBEFORE NVARCHAR2(500) NULL,OPERATETABLE NVARCHAR2(50) NULL,BASEOUGUID NVARCHAR2(50) NULL,OUGUID NVARCHAR2(50) NULL,MACADDRESS NVARCHAR2(50) NULL,RECORDDATE DATE NULL,USERDISPLAYNAME NVARCHAR2(50) NULL,MODULEGUID NVARCHAR2(50) NULL,MODULECODE NVARCHAR2(50) NULL,MODULENAME NVARCHAR2(50) NULL,MODULEURL NVARCHAR2(200) NULL,SERVICENAME NVARCHAR2(50) NULL,SERVICEMETHODNAME NVARCHAR2(50) NULL,PROCESSTYPE NVARCHAR2(50) NULL,ATTACHGUID NVARCHAR2(50) NULL,CONTENT NVARCHAR2(500) NULL,LOGTYPE NVARCHAR2(50) NULL)';
--添加数据表对应的触发器TG_FRAME_LOG(框架日志)
select count(*) into IsExist from  user_triggers where trigger_name = 'TG_FRAME_LOG';
if (IsExist > 0) then
dbms_output.put_line('已存在触发器TG_FRAME_LOG');
else
execute   immediate
'CREATE OR REPLACE TRIGGER TG_FRAME_LOG
BEFORE INSERT ON Frame_Log
FOR EACH ROW
BEGIN
SELECT SQ_FRAME_LOG.NEXTVAL INTO :NEW.ROW_ID FROM DUAL;
END;';
end if;
--添加数据表对应的序列SQ_FRAME_LOG(框架日志)
select count(*) into IsExist from  user_sequences where sequence_name = 'SQ_FRAME_LOG';
if (IsExist > 0) then
dbms_output.put_line('已存在序列SQ_FRAME_LOG');
else
execute   immediate 'CREATE sequence  SQ_FRAME_LOG minvalue 1
 maxvalue 999999999999999999999999999
 start with 1
 increment by 1
 cache 20';
end if;
end;

--记录数据表frame_log_appender(frame_log_appender)
declare IsExist number;
begin
--创建数据表FRAME_LOG_APPENDER
execute immediate  ' CREATE TABLE FRAME_LOG_APPENDER(BELONGXIAQUCODE NVARCHAR2(50) NULL,OPERATEUSERNAME NVARCHAR2(50) NULL,OPERATEDATE DATE NULL,ROW_ID NUMBER(10) NULL,YEARFLAG NVARCHAR2(4) NULL,ROWGUID NVARCHAR2(50) NOT NULL constraint pk_ROWGUID PRIMARY KEY,APPENDERNAME NVARCHAR2(50) NULL,APPENDERCLASS NVARCHAR2(100) NULL,FILEPATH NVARCHAR2(100) NULL,CONVERSIONPATTERN NVARCHAR2(100) NULL,DSNAME NVARCHAR2(100) NULL,EXPORTTYPE NVARCHAR2(50) NULL)';
--添加数据表对应的触发器TG_FRAME_LOG_APPENDER(frame_log_appender)
select count(*) into IsExist from  user_triggers where trigger_name = 'TG_FRAME_LOG_APPENDER';
if (IsExist > 0) then
dbms_output.put_line('已存在触发器TG_FRAME_LOG_APPENDER');
else
execute   immediate
'CREATE OR REPLACE TRIGGER TG_FRAME_LOG_APPENDER
BEFORE INSERT ON frame_log_appender
FOR EACH ROW
BEGIN
SELECT SQ_FRAME_LOG_APPENDER.NEXTVAL INTO :NEW.ROW_ID FROM DUAL;
END;
';end if;
--添加数据表对应的序列SQ_FRAME_LOG_APPENDER(frame_log_appender)
select count(*) into IsExist from  user_sequences where sequence_name = 'SQ_FRAME_LOG_APPENDER';
if (IsExist > 0) then
dbms_output.put_line('已存在序列SQ_FRAME_LOG_APPENDER');
else
execute   immediate 'CREATE sequence  SQ_FRAME_LOG_APPENDER minvalue 1
 maxvalue 999999999999999999999999999
 start with 1
 increment by 1
 cache 20
 ';
end if;
end;

--记录数据表FRAME_LOG_LEVEL(Frame_log_level)
declare IsExist number;
begin
--创建数据表FRAME_LOG_LEVEL
execute immediate  ' CREATE TABLE FRAME_LOG_LEVEL(LEVELGUID NVARCHAR2(50) NOT NULL constraint pk_LEVELGUID PRIMARY KEY,
ROW_ID NUMBER(38) NULL,
LOGNAME NVARCHAR2(100) NULL,
LOGLEVEL NVARCHAR2(5) NULL,
GENERATEDATE DATE NULL,ISWORKING NUMBER(38) NULL,DESCRIPTION NVARCHAR2(500) NULL,APPENDERREF NVARCHAR2(500) NULL)';



--添加数据表对应的触发器TG_FRAME_LOG_LEVEL(Frame_log_level)
select count(*) into IsExist from  user_triggers where trigger_name = 'TG_FRAME_LOG_LEVEL';
if (IsExist > 0) then
dbms_output.put_line('已存在触发器TG_FRAME_LOG_LEVEL');
else
execute   immediate
'CREATE OR REPLACE TRIGGER TG_FRAME_LOG_LEVEL
BEFORE INSERT ON FRAME_LOG_LEVEL
FOR EACH ROW
BEGIN
SELECT SQ_FRAME_LOG_LEVEL.NEXTVAL INTO :NEW.ROW_ID FROM DUAL;
END;
';end if;
--添加数据表对应的序列SQ_FRAME_LOG_LEVEL(Frame_log_level)
select count(*) into IsExist from  user_sequences where sequence_name = 'SQ_FRAME_LOG_LEVEL';
if (IsExist > 0) then
dbms_output.put_line('已存在序列SQ_FRAME_LOG_LEVEL');
else
execute   immediate 'CREATE sequence  SQ_FRAME_LOG_LEVEL minvalue 1
 maxvalue 999999999999999999999999999
 start with 1
 increment by 1
 cache 20
 ';
end if;
end;


