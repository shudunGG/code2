--流程配置 新增流程代理人--刘飞龙
CREATE sequence  SQ_FRAME_COMMISSIONSET_HANDLE minvalue 1 maxvalue 999999999999999999999999999 start with 1 increment by 1 cache 20;
CREATE OR REPLACE TRIGGER TG_FRAME_COMMISSIONSET_HANDLE BEFORE INSERT ON FRAME_COMMISSIONSET_HANDLE FOR EACH ROW BEGIN SELECT SQ_FRAME_COMMISSIONSET_HANDLE.NEXTVAL INTO :NEW.ROW_ID FROM DUAL;

