{
  "name": "ejs.m7.mobileframe.oa",
  "version": "7.1.6",
  "description": "适用于传统批量项目开发，与业务结合的前端框架",
  "author": "dailc",
  "license": "MIT",
  "scripts": {
    "build": "node_modules/.bin/gulp default",
    "watch": "node_modules/.bin/gulp watch",
    "dev": "npm run build && npm run watch",
    "test:eslint": "npm run eslint:report",
    "eslint": "node_modules/.bin/eslint ./js ./pages",
    "eslint:fix": "node_modules/.bin/eslint ./js ./pages --fix",
    "eslint:report": "node_modules/.bin/eslint ./js ./pages -f html -o ./_report/eslint.html"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.10",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^4.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.1",
    "gulp-zip": "^4.2.0",
    "jszip": "^3.1.3",
    "merge-stream": "^1.0.1",
    "mocha": "^5.2.0"
  }
}
